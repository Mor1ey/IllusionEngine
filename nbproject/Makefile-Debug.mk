# Link Libraries and Options
LDLIBSOPTIONS=-L/usr/include/SDL -lSDL -lSDLmain -lguichan

# Build Targets
.build-conf: ${BUILD_SUBPROJECTS}
	"${MAKE}"  -f nbproject/Makefile-Debug.mk dist/Debug/GNU-Linux-x86/illusionengine

dist/Debug/GNU-Linux-x86/illusionengine: ${OBJECTFILES}
	${MKDIR} -p dist/Debug/GNU-Linux-x86
	g++ -o ${CND_DISTDIR}/${CND_CONF}/${CND_PLATFORM}/illusionengine -s ${OBJECTFILES} ${LDLIBSOPTIONS} 

${OBJECTDIR}/src/main.o: src/main.cpp 
	${MKDIR} -p ${OBJECTDIR}/src
	${RM} $@.d
	$(COMPILE.cc) -g -I/usr/include -I/usr/include/guichan -I/usr/include/guichan/sdl -I/usr/include/SDL -MMD -MP -MF $@.d -o ${OBJECTDIR}/src/main.o src/main.cpp

